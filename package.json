{
  "name": "microdi-js",
  "version": "0.1.3",
  "description": "A tiny DI container",
  "keywords": [
    "di",
    "dependency injection",
    "inversion of control",
    "ioc",
    "container",
    "unity",
    "object builder"
  ],
  "scripts": {
    "start": "npm run test",
    "test": "karma start",
    "test:ci": "karma start --single-run",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:prod": "webpack -p --config webpack.prod.config.js"
  },
  "main": "dist/microdi.js",
  "author": "Dev Shop Limited",
  "license": "Apache-2.0",
  "repository": "https://github.com/KeithWoods/microdi-js.git",
  "typings": "./microdi-js.d.ts",
  "devDependencies": {
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "jasmine-core": "2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
